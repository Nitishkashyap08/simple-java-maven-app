pipeline{
    agent {label 'agent-server'}
    tools{
        maven 'mymaven'
    }
    stages{
        stage('clean'){
            steps{
                sh 'mvn clean install'
            }
        }
        stage('test'){
            steps{
                sh 'mvn test'
            }
        }
        stage('deploy'){
            steps{
             sh './deploy.sh'
        }
    }
}
}
