pipeline {
    agent any

    tools {
        maven 'mymaven'
    }

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/Nitishkashyap08/simple-java-maven-app.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}
