pipeline{
    agent{
        label 'dev-server'
    }
    tools{
        maven '3.8.8'
    }
    environment{
        NAME = "NITISH"
    }
    parameters {
  choice choices: ['VIRAT', 'KOHLI', 'SHARMA'], name: 'FNAME'
}
 stages {
    stage('install package'){
    steps{
        sh 'mvn clean package'
    }
    }
   stage('parallel test'){
    parallel{
        stage('test a'){
            steps{
            echo 'this is test a stage'
        }
    }
    
        stage('test b'){
            steps{
            echo 'this is test b stage'
            }
        }
    }
   }
    stage('deploy'){
        steps{
        echo "you build succesfully by $NAME ${params.FNAME}"
        }
    }
 }

}