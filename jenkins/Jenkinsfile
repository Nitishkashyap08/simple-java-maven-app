pipeline{
  agent any
 tools {
  maven 'Maven 3.6.3'  
}

  stages{
    stage('build'){
      steps{
         sh 'mvn clean install'
        echo 'this is build stage'
      }
    }
    stage('test'){
      steps{
        sh 'mvn test'
        echo 'this is test stage'
        
      }
    }
    stage('deploy'){
      steps{
      echo 'this is deploy stage'
      
      }
    }
  }
}